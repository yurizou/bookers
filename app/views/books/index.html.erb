<h1>Books</h1>


<table>
  <thead>
    <tr>
      <th>title</th>
      <th>body</th>
      <th colspan="3"></th>
    </tr>
  </thead>
</table>

<h2>New book</h2>

<%= form_with model: @list do |f| %>
<h4>Title</h4>
 <%= f.text_field :title %>
 <h4>Body</h4>
 <%= f.text_area :body %>
 <%= f.submit 'Create Book' %>
 

<% end %>